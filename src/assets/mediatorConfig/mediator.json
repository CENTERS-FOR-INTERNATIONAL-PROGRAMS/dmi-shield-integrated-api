{
    "urn": "urn:mediator:tutorial_mssql",
    "version": "1.0.0",
    "name": "MSSQL Mediator",
    "description": "MSSQL Mediator",
    "defaultChannelConfig": [
        {
            "name": "MSSQL Mediator",
            "urlPattern": "^/api/v1/documents$",
            "routes": [
                {
                    "name": "MSSQL Mediator Route",
                    "host": "cholera",
                    "port": "3000",
                    "primary": true,
                    "type": "http"
                }
            ],
            "allow": ["admin"],
            "methods": ["GET", "POST"],
            "type": "http"
        },

        {
            "name": "EBS Mediator",
            "urlPattern": "^/api/v1/ebs$",
            "routes": [
                {
                    "name": "EBS Mediator Route",
                    "host": "host.docker.internal",
                    "port": "3000",
                    "primary": true,
                    "type": "http"
                }
            ],
            "allow": ["admin"],
            "methods": ["GET"],
            "type": "http"
        }
    ],
    "endpoints": [
        {
            "name": "MSSQL Mediator Endpoint",
            "host": "host.docker.internal",
            "port": "3000",
            "primary": true,
            "type": "http"
        }
    ],
    "configDefs": [
        {
            "param": "tutorial",
            "displayName": "Tutorial variables",
            "description": "Some variables to demonstrate fetching OpenHIM mediator config",
            "type": "struct",
            "array": false,
            "template": [
                {
                    "param": "variable_1",
                    "displayName": "Variable 1",
                    "description": "First Variable",
                    "type": "string"
                },
                {
                    "param": "variable_2",
                    "displayName": "Variable 2",
                    "description": "Second Variable",
                    "type": "string"
                }
            ]
        }
    ]
}